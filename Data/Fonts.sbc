    <Font>
      <Id>
        <TypeId>FontDefinition</TypeId>
        <SubtypeId>BI_Gray</SubtypeId>
      </Id>
      <ColorMask>
        <X>125</X>
        <Y>125</Y>
        <Z>125</Z>
      </ColorMask>
      <Resources>
        <Resource Path="Fonts\white_shadow\FontDataPA.xml"/>
