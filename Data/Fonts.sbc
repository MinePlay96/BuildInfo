            <Id>
                <TypeId>FontDefinition</TypeId>
                <SubtypeId>BI_Gray</SubtypeId>
            </Id>
            <Public>false</Public>
            <ColorMask>
                <X>125</X>
                <Y>125</Y>
                <Z>125</Z>
            </ColorMask>
            <Resources>
