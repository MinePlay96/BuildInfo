<?xml version="1.0"?>
<Definitions xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Fonts>
        <!-- Custom versions of existing fonts to have more colors and use shadowed version. -->
        <Font>
            <Id>
                <TypeId>FontDefinition</TypeId>
                <SubtypeId>BI_SEOutlined</SubtypeId>
            </Id>
            <Public>false</Public> <!-- do not show in LCD fonts list and such -->
            <Path>Fonts\BI_SEOutlined\FontDataPA.xml</Path> <!-- only tag that works for mod paths -->
            <LanguageSpecificDefinitions>
                <LanguageDefinition Language="ChineseChina">
                    <Resources>
                        <Resource Path="Fonts\white_shadow\FontDataCH.xml" SwappedPath="Fonts\white_shadow\FontDataSwappedCH.xml"/>
                    </Resources>
                </LanguageDefinition>
                <LanguageDefinition Language="Japanese">
                    <Resources>
                        <Resource Path="Fonts\white_shadow\FontDataJP.xml" SwappedPath="Fonts\white_shadow\FontDataSwappedJP.xml"/>
                    </Resources>
                </LanguageDefinition>
            </LanguageSpecificDefinitions>
        </Font>
        <Font>
            <Id>
                <TypeId>FontDefinition</TypeId>
                <SubtypeId>BI_Green</SubtypeId>
            </Id>
            <Public>false</Public>
            <ColorMask>
                <X>100</X>
                <Y>240</Y>
                <Z>100</Z>
            </ColorMask>
            <Resources>
                <Resource Path="Fonts\white_shadow\FontDataPA.xml" SwappedPath="Fonts\white_shadow\FontDataSwappedPA.xml"/>
            </Resources>
            <LanguageSpecificDefinitions>
                <LanguageDefinition Language="ChineseChina">
                    <Resources>
